{
  "name": "insight_xr",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:leaderboard\" --names \"VITE,JAVA\" --prefix-colors \"cyan,green\" --kill-others-on-fail",
    "dev:frontend": "vite",
    "dev:leaderboard": "cd java-leaderboard && powershell -NoProfile -ExecutionPolicy Bypass -File test-run.ps1",
    "build": "tsc && vite build",
    "build:java": "cd java-leaderboard && mvn clean package -DskipTests",
    "preview": "vite preview",
    "leaderboard": "cd java-leaderboard && powershell -NoProfile -ExecutionPolicy Bypass -File test-run.ps1",
    "test": "echo \"No tests configured yet\" && exit 0"
  },
  "dependencies": {
    "@google/genai": "^0.15.0",
    "firebase": "^10.12.2",
    "three": "^0.167.0"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "concurrently": "^9.2.1",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
}
